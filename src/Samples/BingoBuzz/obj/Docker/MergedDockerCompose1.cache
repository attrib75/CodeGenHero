      name: nat
services:
  codegenhero.bingobuzz.webui:
    build:
      context: C:\Code\CodeGenHero\src\Samples\BingoBuzz
      dockerfile: CodeGenHero.BingoBuzz.WebUI\Dockerfile
      target: base
    entrypoint: C:\\remote_debugger\\x64\\msvsmon.exe /noauth /anyuser /silent /nostatus
      /noclrwarn /nosecuritywarn /nofirewallwarn /nowowwarn /timeout:2147483646
