    - published: 50388
      target: 80
    - published: 44350
      target: 443
    volumes:
    - C:\Users\Matt\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\Matt\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
    - C:\Code\CodeGenHero\src\Samples\BingoBuzz\CodeGenHero.BingoBuzz.WebUI:C:\app:rw
    - C:\Users\Matt\onecoremsvsmon\15.0.27703.2026:C:\remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:c:\.nuget\fallbackpackages:ro
    - C:\Users\Matt\.nuget\packages:c:\.nuget\packages:ro
